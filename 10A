10A)
import java.sql.*;
public class Student {
	public static void main(String[] args) {
		String url="jdbc:mysql://localhost:3306/qn10";
		String user="root";
		String pass="";
		try {
			Class.forName("com.mysql.jdbc.Driver");
			Connection conn=DriverManager.getConnection(url,user,pass);
			String s1="select * from student where entry='CET' and branch='CSE'";
			PreparedStatement ps1=conn.prepareStatement(s1);
			ResultSet rs1=ps1.executeQuery();
			System.out.println("Students with CET in CSE branch: ");
			while (rs1.next()) {
				String name=rs1.getString("name");
				String usn=rs1.getString("usn");
				String branch=rs1.getString("branch");
				String entry=rs1.getString("entry");
				System.out.println("\nName: "+name+"\nUSN: "+usn+"\nBranch: "+branch+"\nExam:"+entry);
			}
			rs1.close();
			ps1.close();
			conn.close();
		}
		catch (Exception e) {
			e.printStackTrace();
		}
	}
}
